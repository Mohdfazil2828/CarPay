<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parkingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ActionBar">
        <activity android:name=".activity.activities.PaymentDoneActivity"></activity>
        <activity
            android:name=".activity.activities.UserTimeStart"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.activities.AdminDashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.activities.AdminRegisterActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.activities.AdminLoginActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.activities.OptionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.activities.UserDashboardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.activities.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.activities.RegisterActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activity.activities.LoginActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
    </application>

</manifest>